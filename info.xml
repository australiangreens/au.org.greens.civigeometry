<?xml version="1.0"?>
<extension key="au.org.greens.civigeometry" type="module">
  <file>civigeometry</file>
  <name>CiviGeometry</name>
  <description>CiviCRM Extension for managing geometries and geometry collection</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamus.lee@greens.org.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/australiangreens/au.org.greens.civigeometry</url>
    <url desc="Documentation">https://github.com/australiangreens/au.org.greens.civigeometry</url>
    <url desc="Support">https://github.com/australiangreens/au.org.greens.civigeometry</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-08-01</releaseDate>
  <version>1.12.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/CiviGeometry</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_CiviGeometry_Upgrader</upgrader>
</extension>
